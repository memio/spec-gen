{
    "name": "memio/spec-gen",
    "license": "MIT",
    "type": "library",
    "description": "phpspec extension for better code generation",
    "keywords": ["phpspec", "extension", "generator", "PHP", "code"],
    "homepage": "http://memio.github.io/spec-gen",
    "authors": [
        {
            "name": "Lo√Øc Faugeron",
            "email": "faugeron.loic@gmail.com",
            "homepage": "http://gnugat.github.io",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Memio\\SpecGen\\": "src/Memio/SpecGen"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Memio\\SpecGen\\Fixtures\\": "fixtures",
            "tests\\Memio\\SpecGen\\": "tests"
        }
    },
    "require": {
        "gnugat/redaktilo": "^1.7",
        "memio/memio": "^2.0",
        "memio/linter": "^2.0",
        "memio/validator": "^2.0",
        "memio/model": "^2.0",
        "memio/pretty-printer": "^2.0",
        "memio/twig-template-engine": "^2.0",
        "php": "^7.0",
        "phpspec/phpspec": "^4.0@rc",
        "symfony/event-dispatcher": "^2.7 || ^3.0 || ^4.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpunit/phpunit": "^6.0"
    }
}
